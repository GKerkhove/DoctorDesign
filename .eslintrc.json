{
    "parser": "babel-eslint",
    "rules": {
        "indent": [
            1,
            2
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            1,
            "unix"
        ],
        "semi": [
            1,
            "always"
        ],
        "no-multiple-empty-lines": [
            1,
            {
                "max": 2
            }
        ],
        "no-lonely-if": 1,
        "no-console": 1,
        "no-unused-vars": 1,
        "comma-dangle": 0
    },
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "modules": true,
        "experimentalObjectRestSpread": true
    }
}
